{
  "name": "coreservice-accounts",
  "description": "Users registering to handle user authentication and related functions such as Kakao Login, Driving License certification.",
  "version": "1.14.9",
  "main": "dist/src/index.js",
  "repository": "https://github.com/orangelab-kr/coreservice-accounts.git",
  "author": "bakjonghun <bakjonghun@hikick.kr>",
  "license": "MIT",
  "scripts": {
    "serve": "yarn serve:dev",
    "serve:dev": "sls offline --stage dev",
    "serve:prod": "sls offline --stage prod",
    "serve:stage": "sls offline --stage stage",
    "invoke:passExtend": "yarn invoke:passExtend:dev",
    "invoke:passExtend:dev": "sls invoke local --stage dev --function passExtendScheduler",
    "invoke:passExtend:prod": "sls invoke local --stage prod --function passExtendScheduler",
    "invoke:passExtend:stage": "sls invoke local --stage stage --function passExtendScheduler",
    "invoke:levelUpdate": "yarn invoke:levelUpdate:dev",
    "invoke:levelUpdate:dev": "sls invoke local --stage dev --function levelUpdateScheduler",
    "invoke:levelUpdate:prod": "sls invoke local --stage prod --function levelUpdateScheduler",
    "invoke:levelUpdate:stage": "sls invoke local --stage stage --function levelUpdateScheduler",
    "migrate": "doppler run --command \"prisma db push\"",
    "postmigrate": "doppler run --command \"node prisma/setup.js\"",
    "premigrate": "doppler run --command \"prisma format\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy": "yarn deploy:stage",
    "deploy:prod": "sls deploy --stage prod",
    "deploy:stage": "sls deploy --stage stage",
    "purge": "yarn purge:stage",
    "purge:prod": "sls remove --stage prod",
    "purge:stage": "sls remove --stage stage"
  },
  "dependencies": {
    "@openauth/kakao": "^0.4.0",
    "@prisma/client": "4.2.1",
    "@sentry/node": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "aws-sdk": "^2.1203.0",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "express": "^4.18.1",
    "firebase-admin": "^11.0.1",
    "got": "^11.8.3",
    "i18n": "^0.15.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "libphonenumber-js": "^1.10.13",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^2.7.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.6",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/i18n": "^0.13.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.13",
    "@types/prettier": "^2.7.0",
    "@types/serverless": "^3.12.8",
    "@types/uuid": "^8.3.4",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "prisma": "4.2.1",
    "serverless": "^3.22.0",
    "serverless-doppler": "^1.0.2",
    "serverless-offline": "^9.2.6",
    "serverless-webpack": "^5.9.0",
    "serverless-webpack-prisma": "^1.1.0",
    "ts-loader": "^9.3.1",
    "typescript": "4.8.2",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^3.0.0"
  }
}
